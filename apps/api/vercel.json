{
	"version": 2,
	"builds": [
		{
			"src": "src/main.vercel.ts",
			"use": "@vercel/node"
		},
		{
			"src": "casbin/**",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "src/main.vercel.ts",
			"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
		}
	]
}
